{
  "manifest_version": 2,
  "name": "Chrome OS Player PoC",
  "version": "0.0.0.0",
  "permissions": [
    "http://localhost:9000/",
    "https://raw.githubusercontent.com/",
    "https://apis.google.com/",
    "https://www.googleapis.com/",
    "https://storage.googleapis.com/",
    {"fileSystem": ["write", "retainEntries", "directory", "requestFileSystem"]},
    "unlimitedStorage",
    "system.cpu",
    "system.memory",
    "system.storage",
    "system.display",
    "serial",
    "webview",
    "storage"
  ],
  "minimum_chrome_version": "52",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.bundle.js"
      ]
    }
  },
  "sockets": {
    "udp": {
      "send": [""],
      "bind": [""],
      "multicastMembership": ""
    },
    "tcp": {
      "connect": [""]
    },
    "tcpServer": {
      "listen": ["*:*"]
    }
 },
  "kiosk_enabled": true,
  "kiosk_only": false
}